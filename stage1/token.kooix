import "diag";

enum S1TokenKind {
  Ident(Text);
  Number(Text);
  String(Text);
  KwFn;
  KwRecord;
  KwEnum;
  KwImport;
  KwLet;
  KwReturn;
  KwIf;
  KwElse;
  KwWhile;
  KwMatch;
  LParen;
  RParen;
  LBrace;
  RBrace;
  LBracket;
  RBracket;
  LAngle;
  RAngle;
  Comma;
  Dot;
  Colon;
  Semicolon;
  Plus;
  Bang;
  Eq;
  EqEq;
  NotEq;
  Arrow;
  FatArrow;
  Lte;
  Gte;
  Eof;
};

record S1Token {
  kind: S1TokenKind;
  span: S1Span;
};
