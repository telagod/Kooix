import "../stdlib/prelude";

fn main() -> Int {
  let n: Int = text_len("abc");
  let a: Option<Int> = text_byte_at("abc", 0);
  let s: Option<Text> = text_slice("abc", 1, 3);

  match a {
    Some(v) => {
      match s {
        Some(t) => {
          if n == 3 {
            if v == 97 {
              if t == "bc" { 0 } else { 1 }
            } else { 1 }
          } else { 1 }
        };
        None => 2;
      }
    };
    None => 3;
  }
};
